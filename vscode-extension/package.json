{
  "name": "shiden",
  "displayName": "Shiden",
  "publisher": "IdiotStudios",
  "repository": {
    "type": "git",
    "url": "https://github.com/IdiotStudios/shiden.git"
  },
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],

  "contributes": {
    "languages": [
      {
        "id": "shiden",
        "aliases": ["Shiden", "shiden"],
        "extensions": [".sd"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "shiden",
        "scopeName": "source.shiden",
        "path": "./syntaxes/shiden.tmLanguage.json"
      }
    ]
  },
  "description": "Syntax highlighting for the Shiden language (TextMate grammar only)",
  "icon": "images/icon.png",
  "homepage": "https://github.com/IdiotStudios/shiden",
  "bugs": {
    "url": "https://github.com/IdiotStudios/shiden/issues"
  },
  "keywords": ["shiden","language","syntax"],
  "license": "MIT"
}